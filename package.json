    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-merge": "^0.17.0",
    "webpack-target-electron-renderer": "^0.4.0",
    "webpack-validator": "^2.2.9"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }

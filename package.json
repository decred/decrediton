{
  "name": "decrediton",
  "version": "0.0.0",
  "description": "GUI for decred.",
  "main": "main.js",
  "scripts": {
    "start": "ENV=development electron ./app/main.js",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "start-hot": "electron -r babel-register -r babel-polyfill ./app/main.development",
    "server": "babel-node server.js",
    "dev": "npm run hot-server -- --start-hot",
    "test": "npm test",
    "watch": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decred/decrediton.git"
  },
  "keywords": [
    "decred",
    "cryptocurrency",
    "wallet"
  ],
  "author": "The Decred Authors",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/decred/decrediton/issues"
  },
  "homepage": "https://github.com/decred/decrediton#readme",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "buffer": "^5.0.1",
    "electron": "^1.4.6",
    "es6-promise": "^4.0.5",
    "font-awesome": "^4.7.0",
    "jquery": "^2.2.3",
    "react": "^15.3.2",
    "react-autocomplete": "^1.0.0-rc2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "watchify": "^3.7.0"
  },
  "engines": {
    "node": "~6.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "cross-env": "^3.1.3",
